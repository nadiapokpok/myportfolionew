  <div id="portfolio">
      <h2>Portfolio</h2>
      <hr id="myport">
  </div>
  <div class='card1'>
    <% for(var i=0; i < project.length; i++) { %>
      <div class="c1">
          <h1 id="projet"><%= project[i].titre %></h1>
          <p><%= project[i].description %></p>
          <img class="impec" src="/image/<%= project[i].image %>" />
      <p>Pour voir <a href="<%= project[i].lien %>" target="_blank">cliquez ici</a></p>
      </div>
    <% } %>

  </div>


  <div id="contact">
    <div id="contacts">
        <h2 id="cm">Contactez-moi</h2>
        <hr id="mycontact" />
    </div>

    <form action="/contact" method="POST">
      <label id="name" for="fname">Nom :</label><br>
      <input type="text" id="fname" name="firstname" placeholder="Your name.."><br>
      <label id="mail" for="email">E-mail :</label><br>
      <input type="text" name="email" id="email" placeholder="Your email"><br>
      <label id="yourtext" for="message">Message :</label><br>
      <textarea id="message" name="message" placeholder="Write something.." style="height:200px"></textarea>
      <input type="submit" name="valider" id="submit">
    </form>
      <a href="http://github.com/nadiapokpok" target="_blank">
      <img id="git" src="image/gitw.png" onmouseover="this.src='image/gitb.png'"
      onmouseout="this.src='image/gitw.png'"></a>

    <!-- <a href="http://github.com/nadiapokpok" target="_blank"><img src="image/git.png"></a>-->
  </div>
